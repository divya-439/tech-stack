   "source": [
    "N=int(input())\n",
    "for i in range(0,N):\n",
    "    for j in range(0,N):\n",
    "        if(i==j):\n",
    "            print(\"A\",end=\"\")\n",
    "        else:\n",
    "            print(\" \",end=\"\")\n",
    "    print()\n"
   ]
